{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "standard": {
      "format": "%(asctime)s [%(levelname)s] %(name)s: %(message)s"
    },
    "json": {
      "format": "{\"timestamp\": \"%(asctime)s\", \"level\": \"%(levelname)s\", \"logger\": \"%(name)s\", \"message\": \"%(message)s\"}"
    }
  },
  "handlers": {
    "default": {
      "level": "INFO",
      "formatter": "json",
      "class": "logging.StreamHandler"
    }
  },
  "loggers": {
    "": {
      "handlers": [
        "default"
      ],
      "level": "INFO",
      "propagate": false
    }
  }
}