{
  "name": "lexgraph-legal-rag",
  "version": "1.0.0",
  "description": "LangGraph-powered multi-agent legal document analysis system",
  "main": "index.js",
  "scripts": {
    "test": "pytest tests/",
    "test:unit": "pytest tests/ -m unit",
    "test:integration": "pytest tests/ -m integration",
    "test:e2e": "pytest tests/ -m e2e",
    "test:performance": "k6 run tests/performance/load-test.js",
    "test:stress": "k6 run tests/performance/stress-test.js",
    "test:coverage": "pytest tests/ --cov=lexgraph_legal_rag --cov-report=html",
    "test:mutation": "mutmut run --paths-to-mutate=src/",
    "lint": "ruff check src/ tests/",
    "lint:fix": "ruff check src/ tests/ --fix",
    "format": "black src/ tests/",
    "format:check": "black src/ tests/ --check",
    "typecheck": "mypy src/",
    "security:scan": "bandit -r src/ -f json -o bandit-report.json",
    "security:deps": "safety check",
    "build": "python -m build",
    "build:docker": "docker build -t lexgraph-legal-rag .",
    "build:docker:prod": "docker build --target production -t lexgraph-legal-rag:prod .",
    "dev": "python -m uvicorn lexgraph_legal_rag.api:create_api --host 0.0.0.0 --port 8000 --reload",
    "start": "python -m uvicorn lexgraph_legal_rag.api:create_api --host 0.0.0.0 --port 8000 --workers 4",
    "docs:serve": "python -m http.server 8080 -d docs/",
    "clean": "find . -type f -name '*.pyc' -delete && find . -type d -name '__pycache__' -delete && rm -rf build/ dist/ *.egg-info/ .coverage htmlcov/ .pytest_cache/",
    "deps:update": "pip-tools compile --upgrade requirements.in",
    "deps:sync": "pip-sync requirements.txt",
    "pre-commit": "pre-commit run --all-files",
    "release": "semantic-release",
    "release:dry": "semantic-release --dry-run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/terragon-labs/lexgraph-legal-rag.git"
  },
  "keywords": [
    "legal",
    "rag",
    "ai",
    "langgraph",
    "multi-agent",
    "semantic-search",
    "document-analysis",
    "legal-tech",
    "nlp",
    "machine-learning"
  ],
  "author": "Terragon Labs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/terragon-labs/lexgraph-legal-rag/issues"
  },
  "homepage": "https://github.com/terragon-labs/lexgraph-legal-rag#readme",
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.6",
    "conventional-changelog-conventionalcommits": "^7.0.2",
    "semantic-release": "^22.0.12"
  },
  "release": {
    "branches": [
      "main",
      {
        "name": "develop",
        "prerelease": "beta"
      }
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "conventionalcommits",
          "releaseRules": [
            {
              "type": "docs",
              "scope": "README",
              "release": "patch"
            },
            {
              "type": "refactor",
              "release": "patch"
            },
            {
              "type": "style",
              "release": false
            }
          ]
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "conventionalcommits",
          "presetConfig": {
            "types": [
              {
                "type": "feat",
                "section": "🚀 Features"
              },
              {
                "type": "fix",
                "section": "🐛 Bug Fixes"
              },
              {
                "type": "perf",
                "section": "⚡ Performance Improvements"
              },
              {
                "type": "revert",
                "section": "🔄 Reverts"
              },
              {
                "type": "docs",
                "section": "📚 Documentation"
              },
              {
                "type": "style",
                "section": "💄 Styles",
                "hidden": true
              },
              {
                "type": "chore",
                "section": "🔧 Miscellaneous Chores",
                "hidden": true
              },
              {
                "type": "refactor",
                "section": "♻️ Code Refactoring"
              },
              {
                "type": "test",
                "section": "✅ Tests"
              },
              {
                "type": "build",
                "section": "🔨 Build System"
              },
              {
                "type": "ci",
                "section": "👷 Continuous Integration"
              },
              {
                "type": "security",
                "section": "🔒 Security"
              }
            ]
          }
        }
      ],
      "@semantic-release/changelog",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "pyproject.toml",
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/github"
    ]
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}